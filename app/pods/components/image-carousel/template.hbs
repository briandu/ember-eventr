<div class="carousel-wrapper">
  <div class="carousel">
    {{#each carousel key="@index" as |image index|}}
    <div class="carousel-items" id="{{index}}">
      <img src="{{image.imageUrl}}">
			<div class='content'>
				<h1>{{image.title}}</h1>
				{{#link-to "event"}}
					<div class="button white-outline large">get tickets</div>
				{{/link-to}}
			</div>
    </div>
    {{/each}}
  </div>
</div>

